        "kriswallsmith/assetic"  : "~1.1",
        "leafo/lessphp"          : "~0.4.0",
        "mheap/silex-assetic"    : "1.0.4",
        "monolog/monolog"        : "~1.6",
        "silex/silex"            : "1.2.3",
        "silex/web-profiler"     : "1.0.4",
